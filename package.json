{
  "name": "@ask-utils/isp",
  "version": "1.0.1",
  "description": "Alexa ISP utilities",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "rollup -c",
    "tsc": "tsc",
    "test": "jest",
    "lint": "eslint 'libs/**/*.{ts,tsx}'",
    "doc": "typedoc --out docs libs/",
    "postversion": "git push origin $(git describe --abbrev=0 --tags)",
    "test:watch": "jest --watch",
    "test:dev": "jest --watch --silent=false --verbose false --coverage"
  },
  "bugs": {
    "url": "https://github.com/ask-utils/isp/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ask-utils/isp.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "Alexa",
    "ASK SDK",
    "ISP",
    "ask-utils"
  ],
  "author": "Hidetaka Okamoto <info@wp-kyoto.net> (https://wp-kyoto.net)",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.12",
    "@types/node": "^10.12.18",
    "@types/uuid": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^1.1.1",
    "@typescript-eslint/parser": "^1.1.1",
    "@typescript-eslint/typescript-estree": "^1.1.1",
    "ask-sdk": "^2.5.2",
    "ask-sdk-core": "^2.5.2",
    "ask-sdk-model": "^1.15.1",
    "ask-utils": "^1.6.0",
    "axios": "^0.18.0",
    "eslint": "^5.15.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^23.6.0",
    "moment": "^2.24.0",
    "rollup": "^1.12.3",
    "rollup-plugin-typescript2": "^0.20.1",
    "ts-jest": "^23.10.5",
    "typedoc": "^0.14.1",
    "typescript": "^3.3.3333"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/__tests__/*/*.+(ts|tsx|js)"
    ]
  },
  "homepage": "https://github.com/ask-utils/ispt#readme",
  "directories": {
    "doc": "docs"
  }
}
